<form autocomplete="off">
    <header class="sheet-header">
        <img class="profile-img" src="{{item.img}}" data-edit="img" title="{{item.name}}"/>
        <div class="header-fields">
            <h1 class="charname"><input name="name" type="text" value="{{item.name}}" placeholder="Name"/></h1>
        </div>
    </header>
    <div class="form-group">
        <label for="data.caracteristic">Caractéristique associée : </label>
        <select name="data.caracteristic" id="data.caracteristic" class="flex0">
        {{#select data.caracteristic}}
            {{selectOptions caracteristics selected=value nameAttr=key}}
        {{/select}}
        </select>
    </div>
    
    {{log this}}
</form>